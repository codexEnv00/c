    "ReportService.Protocols": {
      "kind": "entity",
      "doc": "Aspect to capture changes by user and name\n\nSee https://cap.cloud.sap/docs/cds/common#aspect-managed",
      "projection": {
        "from": {
          "ref": [
            "sicredi.db.entities.Protocols"
          ]
        }
      },
      "elements": {
        "ID": {
          "key": true,
          "type": "cds.UUID"
        },
        "createdAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "@UI.HiddenFilter": true,
          "@UI.ExcludeFromNavigationContext": true,
          "@Core.Immutable": true,
          "@title": "{i18n>CreatedAt}",
          "@readonly": true,
          "type": "cds.Timestamp"
        },
        "createdBy": {
          "doc": "Canonical user ID",
          "@cds.on.insert": {
            "=": "$user"
          },
          "@UI.HiddenFilter": true,
          "@UI.ExcludeFromNavigationContext": true,
          "@Core.Immutable": true,
          "@title": "{i18n>CreatedBy}",
          "@readonly": true,
          "@description": "{i18n>UserID.Description}",
          "type": "User",
          "length": 255
        },
        "modifiedAt": {
          "@cds.on.insert": {
            "=": "$now"
          },
          "@cds.on.update": {
            "=": "$now"
          },
          "@UI.HiddenFilter": true,
          "@UI.ExcludeFromNavigationContext": true,
          "@title": "{i18n>ChangedAt}",
          "@readonly": true,
          "type": "cds.Timestamp"
        },
        "modifiedBy": {
          "doc": "Canonical user ID",
          "@cds.on.insert": {
            "=": "$user"
          },
          "@cds.on.update": {
            "=": "$user"
          },
          "@UI.HiddenFilter": true,
          "@UI.ExcludeFromNavigationContext": true,
          "@title": "{i18n>ChangedBy}",
          "@readonly": true,
          "@description": "{i18n>UserID.Description}",
          "type": "User",
          "length": 255
        },
        "protocol": {
          "type": "cds.String"
        },
        "cadoc": {
          "type": "cds.String"
        },
        "status": {
          "type": "sicredi.db.entities.Status"
        },
        "segment": {
          "type": "cds.Association",
          "cardinality": {
            "max": 1
          },
          "target": "sicredi.db.entities.Segments",
          "keys": [
            {
              "ref": [
                "ID"
              ]
            }
          ]
        },
        "details": {
          "type": "cds.Association",
          "cardinality": {
            "max": "*"
          },
          "target": "ReportService.ProtocolDetails",
          "on": [
            {
              "ref": [
                "details",
                "protocol"
              ]
            },
            "=",
            {
              "ref": [
                "$self"
              ]
            }
          ]
        }
      }
    },
